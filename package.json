		"@codemirror/view": "^6.36.7",
		"@testing-library/jest-dom": "^6.8.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.0.14",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@types/sortablejs": "^1.15.8",
		"@typescript-eslint/eslint-plugin": "8.37.0",
